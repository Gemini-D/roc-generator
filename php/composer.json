{
    "name": "hyperf/roc-generator",
    "description": "The Generator for ROC RPC.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "ROC",
        "RPC"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=8.1",
        "google/protobuf": "^3.22",
        "hyperf/command": "^3.0",
        "hyperf/config": "^3.0",
        "hyperf/di": "^3.0",
        "hyperf/event": "^3.0",
        "hyperf/framework": "^3.0",
        "hyperf/phar": "^3.0",
        "symfony/process": "^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\ROCGenerator\\": "src/",
            "Google\\": "src/Protobuf/Google/",
            "GPBMetadata\\": "src/Protobuf/GPBMetadata/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    }
}
